<div class="page-wrapper wall-view">
  <div class="page-body">
    <div class="container-xl">
      <div class="row row-cards">
        <!-- col wall tiling -->
        <div class="col-10">
          <div class="row row-cards wall-tiling">
            <!-- col (1b + 3s) uniform height -->
            <div class="col-12">
              <div class="row row-cards">
                <!-- the big one -->
                {{ with $wall_item := (index $.wall.Items 0) }}
                <div class="col-9">
                  <div class="card wall-panel">
                    <div class="card-header row row-cards">
                      <span class="col-auto status-indicator status-off wall-panel-status">
                        <span class="status-indicator-circle"></span>
                        <span class="status-indicator-circle"></span>
                        <span class="status-indicator-circle"></span>
                      </span>
                      <span class="col text-truncate wall-panel-title"></span>

                      <div class="col-auto btn-list">
                        <button class="btn btn-icon wall-panel-reload">
                          <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-reload"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M19.933 13.041a8 8 0 1 1 -9.925 -8.788c3.899 -1 7.935 1.007 9.425 4.747" /><path d="M20 4v5h-5" /></svg>
                        </button>
                        <button class="btn btn-icon wall-panel-info">
                          <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-info-square"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M12 9h.01" /><path d="M3 5a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v14a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2v-14z" /><path d="M11 12h1v4h1" /></svg>
                        </button>
                      </div>
                    </div>

                    <div class="card-body">
                      <fu-default-live-stream data-fu-src="{{ $wall_item.TempRelayURL }}"></fu-default-live-stream>
                    </div>
                  </div>
                </div>
                {{ end }}

                <!-- col for the 3 on the side of the big one -->
                <div class="col-3">
                  <div class="row row-cards">
                    {{ with $sliced_wall_items := (wtf_wallItemSlice $.wall.Items 1 4) }}
                    {{ range $idx, $wall_item := $sliced_wall_items }}
                    <div class="col-12">
                      <div class="card wall-panel">
                        <div class="card-header row row-cards">
                          <span class="col-auto status-indicator status-off wall-panel-status">
                            <span class="status-indicator-circle"></span>
                            <span class="status-indicator-circle"></span>
                            <span class="status-indicator-circle"></span>
                          </span>
                          <span class="col text-truncate wall-panel-title"></span>
        
                          <div class="col-auto btn-list">
                            <button class="btn btn-icon wall-panel-reload">
                              <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-reload"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M19.933 13.041a8 8 0 1 1 -9.925 -8.788c3.899 -1 7.935 1.007 9.425 4.747" /><path d="M20 4v5h-5" /></svg>
                            </button>
                            <button class="btn btn-icon wall-panel-info">
                              <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-info-square"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M12 9h.01" /><path d="M3 5a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v14a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2v-14z" /><path d="M11 12h1v4h1" /></svg>
                            </button>
                          </div>
                        </div>

                        <div class="card-body">
                          <fu-default-live-stream class="scale-85" data-fu-src="{{ $wall_item.TempRelayURL }}"></fu-default-live-stream>
                        </div>
                      </div>
                    </div>
                    {{ end }}
                    {{ end }}
                  </div>
                </div>
              </div>
            </div>

            <!-- the bottom 4 -->
            {{ with $sliced_wall_items := (wtf_wallItemSlice $.wall.Items 4 8) }}
            {{ range $idx, $wall_item := $sliced_wall_items }}
            <div class="col-3">
              <div class="card wall-panel">
                <div class="card-header row row-cards">
                  <span class="col-auto status-indicator status-off wall-panel-status">
                    <span class="status-indicator-circle"></span>
                    <span class="status-indicator-circle"></span>
                    <span class="status-indicator-circle"></span>
                  </span>
                  <span class="col text-truncate wall-panel-title"></span>

                  <div class="col-auto btn-list">
                    <button class="btn btn-icon wall-panel-reload">
                      <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-reload"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M19.933 13.041a8 8 0 1 1 -9.925 -8.788c3.899 -1 7.935 1.007 9.425 4.747" /><path d="M20 4v5h-5" /></svg>
                    </button>
                    <button class="btn btn-icon wall-panel-info">
                      <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-info-square"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M12 9h.01" /><path d="M3 5a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v14a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2v-14z" /><path d="M11 12h1v4h1" /></svg>
                    </button>
                  </div>
                </div>

                <div class="card-body">
                  <fu-default-live-stream data-fu-src="{{ $wall_item.TempRelayURL }}"></fu-default-live-stream>
                </div>
              </div>
            </div>
            {{ end }}
            {{ end }}
          </div>
        </div>

        <!-- col wall sidebar -->
        <div class="col-2">
          <div class="row row-cards wall-sidebar">
            <div class="col-12">
              <div class="card wall-nav">
                <div class="card-header row row-cards">
                  <div class="btn-list col-12">
                    <button class="btn" onclick="nsCt.fn_goToListing();">
                      <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-list"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M9 6l11 0" /><path d="M9 12l11 0" /><path d="M9 18l11 0" /><path d="M5 6l0 .01" /><path d="M5 12l0 .01" /><path d="M5 18l0 .01" /></svg>
                      Listing
                    </button>
                    <button class="btn" onclick="nsCt.fn_goToEdit();">
                      <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-edit"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M7 7h-1a2 2 0 0 0 -2 2v9a2 2 0 0 0 2 2h9a2 2 0 0 0 2 -2v-1" /><path d="M20.385 6.585a2.1 2.1 0 0 0 -2.97 -2.97l-8.415 8.385v3h3l8.385 -8.415z" /><path d="M16 5l3 3" /></svg>
                      Edit
                    </button>
  
                    <button class="btn btn-icon ms-auto" onclick="window.location.reload();">
                      <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-reload"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M19.933 13.041a8 8 0 1 1 -9.925 -8.788c3.899 -1 7.935 1.007 9.425 4.747" /><path d="M20 4v5h-5" /></svg>
                    </button>
                  </div>
                  <!-- <div class="btn-list col-12 mt-1">
                    <button class="btn ms-auto" onclick="window.location.reload();">
                      <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-reload"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M19.933 13.041a8 8 0 1 1 -9.925 -8.788c3.899 -1 7.935 1.007 9.425 4.747" /><path d="M20 4v5h-5" /></svg>
                      Reload
                    </button>
                  </div> -->
                </div>
              </div>
            </div>

            <div class="col-12">
              <div class="card wall-info">
                <div class="card-body">
                  <h3 class="card-title">{{ $.wall.Code }}</h3>
                  <h3 class="card-title">{{ $.wall.Name }}</h3>
                  <p class="card-subtitle">{{ $.wall.Note }}</p>
                </div>
              </div>
            </div>

            <div class="col-12">
              <div class="card wall-tabs">
                <div class="card-header">
                  <ul class="nav nav-tabs card-header-tabs" data-bs-toggle="tabs" role="tablist">
                    <li class="nav-item" role="presentation">
                      <a class="nav-link active" href="#tab-events" data-bs-toggle="tab" aria-selected="true" role="tab">
                        Events
                      </a>
                    </li>
                    <!-- <li class="nav-item dropdown">
                      <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown</a>
                      <div class="dropdown-menu" style="">
                        <a class="dropdown-item" href="#">
                          Action
                        </a>
                        <a class="dropdown-item" href="#">
                          Another action
                        </a>
                        <a class="dropdown-item" href="#">
                          <span class="animated-dots"></span>
                        </a>
                      </div>
                    </li> -->
                    <li class="nav-item ms-auto" role="presentation">
                      <a class="nav-link" href="#tab-n" data-bs-toggle="tab" aria-selected="false" role="tab" tabindex="-1">
                        <svg  xmlns="http://www.w3.org/2000/svg"  width="24"  height="24"  viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-settings"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M10.325 4.317c.426 -1.756 2.924 -1.756 3.35 0a1.724 1.724 0 0 0 2.573 1.066c1.543 -.94 3.31 .826 2.37 2.37a1.724 1.724 0 0 0 1.065 2.572c1.756 .426 1.756 2.924 0 3.35a1.724 1.724 0 0 0 -1.066 2.573c.94 1.543 -.826 3.31 -2.37 2.37a1.724 1.724 0 0 0 -2.572 1.065c-.426 1.756 -2.924 1.756 -3.35 0a1.724 1.724 0 0 0 -2.573 -1.066c-1.543 .94 -3.31 -.826 -2.37 -2.37a1.724 1.724 0 0 0 -1.065 -2.572c-1.756 -.426 -1.756 -2.924 0 -3.35a1.724 1.724 0 0 0 1.066 -2.573c-.94 -1.543 .826 -3.31 2.37 -2.37c1 .608 2.296 .07 2.572 -1.065z" /><path d="M9 12a3 3 0 1 0 6 0a3 3 0 0 0 -6 0" /></svg>
                      </a>
                    </li>
                  </ul>
                </div>
                <div class="card-body">
                  <div class="tab-content">
                    <div id="tab-events" class="tab-pane active show" role="tabpanel">
                      <h4>Events</h4>
                      <div>Coming soon</div>
                    </div>
                    <div id="tab-n" class="tab-pane" role="tabpanel">
                      <h4>Tab n</h4>
                      <div>lorem ipsum dolor si amet</div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- col wall sidebar -->
      </div>
    </div>
  </div>
</div>

